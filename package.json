{
  "name": "discord-llm-bot",
  "version": "1.0.0",
  "description": "A Discord bot to communicate with LLMs and execute Python code",
  "type": "commonjs",
  "scripts": {
    "lint": "npx eslint src/",
    "check-types": "tsc --noEmit",
    "version": "tsc -v",
    "build": "tsc && mkdir -p dist/scripts && cp src/scripts/* dist/scripts/ || true && cp -r config dist/",
    "start": "node --max-old-space-size=256 dist/src/index.js",
    "start:dev": "nodemon --exec npx ts-node src/index.ts",
    "validate": "npm-run-all --parallel lint test",
    "test": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test jest --runInBand",
    "test:coverage": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test jest --coverage --runInBand",
    "test:debug": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand",
    "test:monitor": "npx cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand --watch",
    "ncu": "ncu"
  },
  "_moduleAliases": {
    "@src": "src",
    "@command": "src/command",
    "@common": "src/common",
    "@config": "src/config",
    "@llm": "src/llm",
    "@message": "src/message",
    "@webhook": "src/webhook",
    "@integrations": "src/integrations",
    "@types": "src/types"
  },
  "keywords": [],
  "author": "matthewhand+dot+au+at+gmail+dot+com",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^2.0.0",
    "@discordjs/voice": "^0.17.0",
    "@eslint/config-array": "^0.17.1",
    "@eslint/object-schema": "^2.1.4",
    "@slack/socket-mode": "^2.0.3",
    "@slack/web-api": "^7.8.0",
    "@types/pino": "^7.0.4",
    "abstract-syntax-tree": "^2.22.0",
    "are-we-there-yet": "^4.0.2",
    "axios": "^1.6.5",
    "config": "^3.3.12",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "discord-api-types": "^0.37.67",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "flowise-sdk": "^1.0.8",
    "form-data": "^4.0.0",
    "gauge": "^5.0.2",
    "glob": "^11.0.0",
    "gpt-tokenizer": "^2.1.2",
    "libsodium-wrappers": "^0.7.9",
    "lru-cache": "^11.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "npmlog": "^7.0.1",
    "openai": "^4.56.1",
    "pino-pretty": "^11.2.2",
    "rimraf": "^6.0.1",
    "serverless-http": "^3.2.0",
    "ts-essentials": "^10.0.2",
    "typeforce": "^1.18.0",
    "uuidv4": "^6.2.13",
    "winston-daily-rotate-file": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.1",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-flow": "^7.24.1",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/chai": "^4.3.19",
    "@types/config": "^3.3.4",
    "@types/convict": "^6.1.6",
    "@types/debug": "^4.1.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/ffmpeg-static": "^3.0.3",
    "@types/form-data": "^2.2.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/mocha": "^10.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.10.7",
    "@types/node-fetch": "^2.6.11",
    "@types/npmlog": "^7.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuidv4": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
