{
  "name": "open-hivemind",
  "version": "1.0.0",
  "description": "A messenger bot to communicate with LLMs",
  "type": "commonjs",
  "bin": {
    "hivemind": "dist/src/cli/HivemindCLI.js"
  },
  "scripts": {
    "lint": "npx eslint src/",
    "check-types": "tsc --noEmit",
    "version": "tsc -v",
    "build": "mkdir -p dist/scripts && cp src/scripts/* dist/scripts/ || true && cp -r config dist/ && node node_modules/.bin/tsc",
    "start": "node --max-old-space-size=256 dist/src/index.js",
    "start:dev": "nodemon --exec npx ts-node src/index.ts",
    "validate": "npm-run-all --parallel lint test",
    "test": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test node -r dotenv/config node_modules/.bin/jest --runInBand",
    "test:coverage": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test node_modules/.bin/jest --coverage --runInBand",
    "test:real": "NODE_ENV=test jest --config jest.real.config.js",
    "test:real:legacy": "./scripts/run-real-tests.sh",
    "test:debug": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand",
    "test:monitor": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand --watch",
    "ncu": "ncu"
  },
  "_moduleAliases": {
    "@src": "dist/src",
    "@command": "dist/src/command",
    "@common": "dist/src/common",
    "@config": "dist/src/config",
    "@auth": "dist/src/auth",
    "@llm": "dist/src/llm",
    "@message": "dist/src/message",
    "@webhook": "dist/src/webhook",
    "@integrations": "dist/src/integrations",
    "@types": "dist/src/types"
  },
  "keywords": [],
  "author": "matthewhand+dot+au+at+gmail+dot+com",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^2.0.0",
    "@discordjs/voice": "^0.17.0",
    "@slack/rtm-api": "^7.0.2",
    "@slack/socket-mode": "^2.0.3",
    "@slack/web-api": "^7.8.0",
    "@slack/webhook": "^7.0.4",
    "@tryfabric/mack": "^1.2.1",
    "@types/commander": "^2.12.0",
    "@types/pino": "^7.0.4",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "abstract-syntax-tree": "^2.22.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.5",
    "bcrypt": "^6.0.0",
    "commander": "^9.5.0",
    "config": "^3.3.12",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "discord-api-types": "^0.37.67",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.2.0",
    "flowise-sdk": "^1.0.8",
    "form-data": "^4.0.0",
    "gpt-tokenizer": "^2.1.2",
    "html-entities": "^2.5.2",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.9",
    "lru-cache": "^11.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.82.0",
    "pino-pretty": "^11.2.2",
    "serverless-http": "^3.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-essentials": "^10.0.2",
    "typeforce": "^1.18.0",
    "uuidv4": "^6.2.13",
    "winston-daily-rotate-file": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-flow": "^7.24.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/bcrypt": "^6.0.0",
    "@types/chai": "^4.3.19",
    "@types/config": "^3.3.4",
    "@types/convict": "^6.1.6",
    "@types/debug": "^4.1.12",
