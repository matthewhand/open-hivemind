    "cross-env": "^7.0.3",
    "eslint": "^9.11.0",
    "jest": "^29.7.0",
    "jest-snapshot": "^29.7.0",
    "mocha": "^10.7.3",
    "natural-compare": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-unused-exports": "^10.1.0",
    "typescript": "^5.6.2",
    "wrangler": "^3.23.0"
  }
}
