    "@types/node": "^22.10.7",
    "@types/node-fetch": "^2.6.11",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuidv4": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "jest-snapshot": "^29.7.0",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-unused-exports": "^10.1.0",
    "typescript": "^5.9.2",
    "wrangler": "^3.23.0"
  }
}
