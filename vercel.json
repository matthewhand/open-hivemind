{
  "version": 2,
  "outputDirectory": "src/client/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false,
    "silent": true
  },
  "git": {
    "ignore": [
      {
        "type": "pull-request"
      }
    ],
    "deploymentHook": {
      "enabled": false
    }
  }
}